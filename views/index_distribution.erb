<div id="index_distributions">

  <table>
    <thead>
    <th>Amount</th>
    <th>Charity</th>
    </thead>
    <tbody>
    <%distributions_array.each do |distribution|%>
      <tr>
        <td>$<%=distribution["amount"].to_i / 100%></td>
        <td><%=distribution["charity"]%></td>
      </tr>
    <%end%>
    </tbody>
  </table>



</div>